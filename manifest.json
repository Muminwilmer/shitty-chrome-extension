{
  "manifest_version": 3,
  "name": "Mumins playkit",
  "version": "1.0",
  "description": "Skip an ad instead of blocking, it also includes random Mumin projects.",
  "permissions": [
    "activeTab",
    "storage"
  ],
  "action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches": ["*://www.youtube.com/*"],
      "js": ["content.js"]
    }
  ]
}