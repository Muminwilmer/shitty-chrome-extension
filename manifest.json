{
  "manifest_version": 3,
  "name": "Mumins toolkit",
  "version": "1.1.0",
  "description": "Skips youtube ads instead of blocking them, also includes random Mumin projects.",
  "permissions": [
    "activeTab",
    "storage"
  ],
  "action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches": ["*://*/*"],
      "js": ["adblocker.js","prideFlagFix.js"]
    }
  ]
}